{
  "imports": {
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@8230f895bad2797e3a677d07183374d17e49e652/"
  },
  "lock": false,
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "cms": "deno task lume cms"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
