{
  "imports": {
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@bb8942c299dd3b4bd7fcb23519f7058704b9430c/"
  },
  "lock": false,
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run --unstable -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "cms": "deno task lume cms"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
