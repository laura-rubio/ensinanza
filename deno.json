{
  "imports": {
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@202fc22ef261b90d8fc086def93e2f758915b05f/"
  },
  "lock": false,
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run --unstable -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "cms": "deno task lume cms"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
