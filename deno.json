{
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.1.0/",
    "https://cdn.jsdelivr.net/gh/lumeland/cms@v0.2.9/": "../lumeland/cms/"
  },
  "lock": false,
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "cms": "deno task lume cms"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
